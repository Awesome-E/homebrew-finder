{
	"name": "Homebrew Finder",
	"description": "A Browser Extension that finds Homebrew Packages for the current website you're on",
	"version": "0.1",
	"permissions": [
		"activeTab",
    "tabs"
	],
	"web_accessible_resources": [],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [],
	"browser_action": {
		"default_title" : "Homebrew Finder",
		"default_icon" : "icons/pack-icon-inactive.png",
		"default_popup": ""
	},
  "icons": {
      "16": "icons/pack-icon.png",
      "32": "icons/pack-icon.png",
      "48": "icons/pack-icon.png"
	},
	"minimum_chrome_version": "80.0.3987",
	"manifest_version": 2
}
